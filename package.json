{
  "name": "plan-my-escape-app",
  "description": "a Web-based editor for making online escape room games.",
  "version": "0.1",
  "engines": {
    "node": "22.x"
  },
  "private": true,
  "dependencies": {
    "@gltf-transform/core": "^4.0.8",
    "@google/model-viewer": "^3.5.0",
    "@leva-ui/plugin-bezier": "^0.9.19",
    "@react-three/fiber": "^8.16.8",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.20.2",
    "leva": "0.9.34",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "18.3.1",
    "react-three-fiber": "^6.0.13",
    "sharp": "^0.33.4",
    "three": "^0.166.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "node esbuild.config.mjs",
    "lint:js": "eslint --quiet app/javascript/src/**/*",
    "lint:ruby": "bundle exec rubocop --parallel -c ./.rubocop.yml",
    "test": "jest app/javascript/tests",
    "cypress:compile": "RAILS_ENV=cypress bundle install && bundle exec rails assets:precompile",
    "cypress:rails": "RAILS_ENV=cypress bundle exec rails db:prepare || bundle exec rails s -e cypress -p 5002 -d",
    "cypress:local": "yarn cypress:compile && yarn cypress:rails",
    "cypress:headless": "export NO_PROXY=localhost && yarn cypress run --e2e --config-file \"./cypress.config.ts\"",
    "cypress:ci": "yarn cypress:local && yarn cypress:headless"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint-react/eslint-plugin": "^1.5.11",
    "@eslint/js": "^9.2.0",
    "@react-three/drei": "^9.109.2",
    "@react-three/gltfjsx": "^4.3.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/react-test-renderer": "^18.3.0",
    "@types/three": "^0.167.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "cypress": "^13.9.0",
    "cypress-esbuild-preprocessor": "sod/cypress-esbuild-preprocessor",
    "esbuild-rails": "^1.0.7",
    "eslint": "^9.5.0",
    "eslint-plugin-cypress": "^3.2.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-internal": "^0.0.1-security",
    "eslint-plugin-security": "^3.0.0",
    "globals": "^15.1.0",
    "gltfjsx": "^6.4.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.7",
    "local-cypress": "^1.2.6",
    "npx": "^10.2.2",
    "path": "^0.12.7",
    "postinstall-postinstall": "^2.1.0",
    "punycode": "^2.3.1",
    "react-test-renderer": "^18.3.1",
    "three-engine-ts-types": "^1.0.36",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.8.0",
    "url": "^0.11.4"
  }
