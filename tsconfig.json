{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "allowJs": true,
    "lib": [
      "es5",
      "dom"
    ],
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "app/javascript/**/*.ts*",
    "app/javascript/**/*.js*"
, "app/javascript/src/PrivateJet.jsx"  ]
}