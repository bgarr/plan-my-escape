{
  "compilerOptions": {
    "module": "es2015",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "es5",
      "dom"
    ],
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "cypress",
      "node"
    ]
  },
  "include": [
    "app/javascript/**/*.ts*"
  ]
}